{
    "name": "boboldehampsink/tagmanager",
    "description": "Plugin that allows you to edit and delete tags.",
    "type": "craft-plugin",
    "version": "2.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "tagmanager"
    ],
    "support": {
        "docs": "https://github.com/boboldehampsink/tagmanager/blob/master/README.md",
        "issues": "https://github.com/boboldehampsink/tagmanager/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bob Olde Hampsink",
            "homepage": "https://github.com/boboldehampsink",
            "email": "b.oldehampsink@itmundi.nl"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "boboldehampsink\\tagmanager\\": "src/"
        }
    },
    "extra": {
        "name": "Tag Manager",
        "handle": "tagmanager",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/boboldehampsink/tagmanager/master/CHANGELOG.md",
        "class": "boboldehampsink\\tagmanager\\TagManager"
    }
}
