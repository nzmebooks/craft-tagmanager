{
    "name": "nzmebooks/craft-tagmanager",
    "description": "Plugin that allows you to edit and delete tags.",
    "type": "craft-plugin",
    "version": "2.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "tagmanager"
    ],
    "support": {
        "docs": "https://github.com/nzmebooks/craft-tagmanager/blob/master/README.md",
        "issues": "https://github.com/nzmebooks/craft-tagmanager/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jason Darwin",
            "homepage": "https://github.com/nzmebooks",
            "email": "mebooks.support@gmail.com"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0-alpha.1",
        "php": "^8.0.2"
    },
    "autoload": {
        "psr-4": {
          "nzmebooks\\tagmanager\\": "src/"
        }
    },
    "extra": {
        "name": "Tag Manager",
        "handle": "tagmanager",
        "hasCpSettings": false,
        "hasCpSection": true,
        "changelogUrl": "https://raw.githubusercontent.com/nzmebooks/craft-tagmanager/master/CHANGELOG.md",
        "class": "nzmebooks\\tagmanager\\TagManager"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "craftcms/rector": "dev-main"
    }
}
